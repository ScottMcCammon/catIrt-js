{
  "name": "catirtlib.wasm",
  "version": "1.0.0",
  "description": "WebAssembly port of catIrt library",
  "main": "catirtlib.js",
  "engines": {
    "node": ">=12.18.4"
  },
  "scripts": {
    "clean": "rm dist/catirtlib.*",
    "build": "emcc -O2 --bind -I ./dist/eigen catirtlib.cpp --pre-js additions.js -o dist/catirtlib.js -s WASM=1 -s MODULARIZE -s EXPORT_NAME='createCatIrtLib'",
    "test": "mocha --exclude 'test/manual*.js'"
  },
  "author": "Scott McCammon <mccammon@uoregon.edu>",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "mathjs": "^7.5.1",
    "mocha": "^8.1.3"
  }
}
